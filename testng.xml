<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="4" parallel= "classes">
  <test name="Test">
  <parameter name = "num1" value= "5" />
  <parameter name = "num2" value = "3"/>
    <classes>
    
      <class name="calculator.Arithmetic">
      
      <methods>
       <exclude name="DivReal"/>
       <include name= "subtract"/>
       <include name = "Multiplication"/>
       </methods>
           
     </class> 
      <class name="calculator.MathFunctions">
      <methods>
       <exclude name="negate"/>
        <exclude name="inverse"/>
         <exclude name="exp"/>
      </methods>
       </class> 
    </classes>
    
  </test> <!-- Test -->
</suite> <!-- Suite -->
